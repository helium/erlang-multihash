{cover_enabled, true}.
{cover_export_enabled, true}.

{erl_opts, [warn_untyped_records, debug_info, warnings_as_errors]}.

{dialyzer, [{warnings, [no_return, no_match, unmatched_returns, error_handling, race_conditions, underspecs]}]}.

{deps, [
        {keccakf1600, "2.0.0"},
        {small_ints, "0.1.0"}
       ]}.
